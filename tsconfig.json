{
    "compilerOptions": {
      "target": "ES6",
      "module": "CommonJS",
      "rootDir": "src",
      "outDir": "dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "sourceMap": true, // Ensure source maps are enabled
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  